<li id='<?php echo $this->menuId . "_item_wrapper_" . $this->item->id;?>' class='menuItem'>
<?php 
	if($this->item->isSelected()){
	    $class="selected";
	}else{
		$class = "unselected";
	}
	
	$menuItemId = $this->menuId . "_item_" . $this->item->id;
    echo $this->item->asHyperlink($menuItemId, $class);
    if($this->submenu != null) {
        echo $this->submenu;
    }
?>
</li>